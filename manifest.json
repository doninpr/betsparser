{
  "manifest_version": 2,
  "name": "Bets Parser",
  "version": "1.0",
  
  "icons": {
    "32": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "js": [ "content.js" ]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "http://losttime.su/*"
  ],

      "browser_action": {
        "default_title": "LostTime",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
	
}